  keywords = {anterior cingulate cortex,cognitive control,dorsolateral prefrontal cortex,error detection,inhibitory interneurons},
  file = {Cerebral Cortex-2014-Shen et al-Anterior Cingulate Cortex Cells Identify Process-Specific Errors of Attentional.pdf:/Users/edeno/Dropbox/Papers/Cerebral Cortex-/2014/Cerebral Cortex-2014-Shen et al-Anterior Cingulate Cortex Cells Identify Process-Specific Errors of Attentional.pdf:application/pdf;Snapshot:/Users/edeno/Library/Application Support/Zotero/Profiles/dwteqqbq.default/zotero/storage/4TEMIPBM/cercor.bhu028.html:},
