  keywords = {action monitoring,Anterior cingulate,error detection,Error-related negativity,event-related potential,response conflict},
  file = {The Journal of Neuroscience-2001-Gehring_Fencsik-Functions of the Medial Frontal Cortex in the Processing of Conflict and Errors.pdf:/Users/edeno/Dropbox/Papers/The Journal of neuroscience-/2001/The Journal of Neuroscience-2001-Gehring_Fencsik-Functions of the Medial Frontal Cortex in the Processing of Conflict and Errors.pdf:application/pdf;Snapshot:/Users/edeno/Library/Application Support/Zotero/Profiles/dwteqqbq.default/zotero/storage/892HZ26P/9430.html:},
