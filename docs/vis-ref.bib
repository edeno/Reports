  urldate = {2014-04-11},
  date = {2012-08-23},
  pages = {572--583},
  file = {Neuron-2012-Ainsworth et al-Rates and Rhythms.pdf:/Users/edeno/Dropbox/Papers/Neuron-/2012/Neuron-2012-Ainsworth et al-Rates and Rhythms.pdf:application/pdf;ScienceDirect Snapshot:/Users/edeno/Library/Application Support/Zotero/Profiles/dwteqqbq.default/zotero/storage/K7K7679H/S089662731200709X.html:}
