  keywords = {corticothalamic,frontal cortex,mediodorsal,thalamocortical,ventral anterior,ventral lateral},
  file = {The Journal of Neuroscience-2002-McFarland_Haber-Thalamic Relay Nuclei of the Basal Ganglia Form Both Reciprocal and.pdf:/Users/edeno/Dropbox/Papers/The Journal of neuroscience-/2002/The Journal of Neuroscience-2002-McFarland_Haber-Thalamic Relay Nuclei of the Basal Ganglia Form Both Reciprocal and.pdf:application/pdf;Snapshot:/Users/edeno/Library/Application Support/Zotero/Profiles/dwteqqbq.default/zotero/storage/P6VJEPCI/8117.html:},
