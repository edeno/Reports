  date = {2000-11-01},
  pages = {271--291},
  langid = {english},
  keywords = {Alpha,attention,cortex,Human Genetics,Neurology,Neurosciences,rhythms,synchrony,Theory of Computation},
  file = {Journal of Computational Neuroscience-2000-Jones et al-Alpha-Frequency Rhythms Desynchronize over Long Cortical Distances.pdf:/Users/edeno/Dropbox/Papers/Journal of Computational Neuroscience-/2000/Journal of Computational Neuroscience-2000-Jones et al-Alpha-Frequency Rhythms Desynchronize over Long Cortical Distances.pdf:application/pdf;Snapshot:/Users/edeno/Library/Application Support/Zotero/Profiles/dwteqqbq.default/zotero/storage/3TKETTNK/10.html:}
