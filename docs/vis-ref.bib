  date = {1996-01-12},
  pages = {7742--7756},
  langid = {english},
  keywords = {dendrite,layer V pyramidal cell,neocortex,short-term plasticity,synchronization,thalamus,voltage-dependent conductances},
  file = {The Journal of Neuroscience-1996-Castro-Alamancos_Connors-Cellular Mechanisms of the Augmenting Response.pdf:/Users/edeno/Dropbox/Papers/The Journal of neuroscience-/1996/The Journal of Neuroscience-1996-Castro-Alamancos_Connors-Cellular Mechanisms of the Augmenting Response.pdf:application/pdf},
