  pages = {43--62},
  keywords = {coherence,Generalized linear model,Hypothesis testing,Local field potential,Oscillations,Point process,rhythms,Spike-field coherence,Spikes,Statistical association},
  file = {Journal of Neuroscience Methods-2013-Lepage et al-A procedure for testing across-condition rhythmic spike-field association change.pdf:/Users/edeno/Dropbox/Papers/Journal of Neuroscience Methods-/2013/Journal of Neuroscience Methods-2013-Lepage et al-A procedure for testing across-condition rhythmic spike-field association change.pdf:application/pdf;ScienceDirect Snapshot:/Users/edeno/Library/Application Support/Zotero/Profiles/dwteqqbq.default/zotero/storage/ZD57T22X/S0165027012004207.html:}
